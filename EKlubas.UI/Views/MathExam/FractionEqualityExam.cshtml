@model IEnumerable<Fraction>

@{
    ViewData["Title"] = "FractionEqualityExam";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>FractionEqualityExam</h2>

<div class="row">
    @foreach (var fraction in Model)
    {
        <div class="col-md-2">
            @Html.Raw(HttpUtility.HtmlDecode(fraction.GetFractionHtml()))
        </div>
    }
</div>
